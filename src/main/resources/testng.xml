<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Ebay OOPs Test Suite" parallel="tests" verbose="3">

<listeners>
	<listener
		class-name="com.ExtentReportListener.ExtentReporterNG" />
		<listener class-name="listner.AllTransformer" />
</listeners>

	<test name="my ebay test chrome" parallel="methods" thread-count="5">
	<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.tests.LoginPageTest"/>
			<class name="com.tests.HomePageTest"/>
		</classes>
	</test>
	
